            // Check affiliate is Awin and has qualified affiliate ID
            if(affiUrl.indexOf(mainObj[j]) > -1 && affiUrl.indexOf('awc') > -1 && affiUrl.indexOf('dcmp=afc') > -1) {   
                for(var k = 0; k < mainObjPlan.length; k++) {
                    // If URL has qualified affiliate ID, push all plans into 'planArr'
                    var planObj = mainObjPlan[k];                
                    planArr.push(planObj);
                }            
            } else if(affiUrl.indexOf(mainObj[j]) > -1) {   
                for(var l = 0; l < mainObjPlan.length; l++) {
                    // If URL has other plans, push all plans into 'planArr'
                    var planObj = mainObjPlan[l];                
                    planArr.push(planObj);
                }            
            }              
        }
    }
    
    if(planArr.length > 0) {
        var encodedArr = btoa(encodeURIComponent(JSON.stringify(planArr)))
        $smcT5.CookieManager.create("smc_affi_plan",encodedArr, 0);    
    }
