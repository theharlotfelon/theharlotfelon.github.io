$smcT5.CustomScripts.TriggerThirdPartyAnalytics = function(type = 'default') {
    if(!type || type === 'default') {
        return false;
    }
    // Optimizely
    window['optimizely'] = window['optimizely'] || [];
    window['optimizely'].push({
      type: "event",
      eventName: (type === 'control' ? 'intentlyControl' : 'intentlyTest'),
      tags: {}
    });
    
    // Adobe Analytics
    if(window.skyTags) {
        window.skyTags.queue.push(['set', { page: { custom: { variant: (type === 'control' ? 'intentlyShownControl' : 'intentlyShownTest') } } }]);
        window.skyTags.queue.push(['event', { type: 'click' }], ['track']);
    }
}

/**
 * D.P - 22/02/2022 - Adding SkyAB DK to return the AB result that Sky setup, current values are 'control-0' and 'test-1'
 **/
$smcT5.CustomScripts.SkyAB = function() {
    if(window && window.intently && window.intently.exp) {
        return window.intently.exp;
    } else {
        return false;
    }
}

$smcT5.CustomScripts.OrderId = function() {
    if(window && window.dtm && window.dtm.orderId) {
        return window.dtm.orderId;
    } else {
        return false;
    }
}

$smcT5.CustomScripts.TagCode = function() {
    (function getAffiliatePrice() {
        $smcT5.CustomScripts.CollectAffiliatePrice();
    })();
    
    (function GetPackageInfo(){
        setInterval(function(){
            var packages = document.querySelectorAll('[data-test-id="broadband-products-page-section"] [role="tablist"] button');
            var packageArr = [];
            if (packages && packages.length > 0){
                for (i = 0; i < packages.length; i++){
                    var pkg = packages[i];
                    var classes = pkg.classList.value;
                    var currentlySelected = false;
                    if (classes.indexOf('active') > -1){
                        currentlySelected = true;
                    }
                    packageArr.push({
                        name : pkg.getAttribute('aria-label'),
                        index : i,
                        currentlySelected : currentlySelected,
                        packagePrice : pkg.querySelector('.c-price__main').innerText
                    });
                }
        
                localStorage.setItem('smc_available_packages', JSON.stringify(packageArr));
            }
        }, 1000);
    })();
    
    (function BasketScanner(){
        var getATC = setInterval(function(){   
            var pricePlanCookie = $smcT5.CookieManager.read('smc_affi_plan');
            var title = document.querySelector('.c-broadband-product-tabs .tab-headers button.tab-header--active .product-tab__header-heading');
            var price = document.querySelector('.c-broadband-product-tabs .tab-headers button.tab-header--active .c-price');
            var duration = document.querySelector('.c-broadband-product-tabs .tab-headers button.tab-header--active .product-tab__header-duration');
            if (title && price && duration && pricePlanCookie){
                var parsed = JSON.parse(decodeURIComponent(atob(pricePlanCookie)));
                var obj = {};
                for(var i = 0; i < parsed.length; i++) {
                    if(parsed[i].name.trim() === title.textContent.trim()) {
                        obj.priceSky = parsed[i].skyPrice;
                        obj.affiSetup = parsed[i].affiSetup;
                        obj.skySetup = parsed[i].skySetup;
                        break;
                    }
                }
                obj.title = title.textContent;
                obj.duration = duration.textContent;
                obj.priceAffi = price.textContent;
                
                var itemEncoded = btoa(encodeURIComponent(JSON.stringify(obj)));
                $smcT5.CookieManager.create('smc_dyn_item', itemEncoded, 0);
                
            } else if (title && price && duration) {
                var objNew = {};
                objNew.title = title.textContent;
                objNew.priceSky = price.textContent;
                objNew.priceAffi = price.textContent;
                objNew.duration = duration.textContent;
                objNew.affiSetup = '£0';
                objNew.skySetup = '£19.95';
                
                var itemEncodedNew = btoa(encodeURIComponent(JSON.stringify(objNew)));
                $smcT5.CookieManager.create('smc_dyn_item', itemEncodedNew, 0);
            }     
                 
        }, 700);
    })();
};

$smcT5.CustomScripts.BroadbandCurrentlySelected = function(){
    var lsitem = localStorage.getItem('smc_available_packages');
    if (!lsitem){
        return false;
    } else if (location.href.indexOf('/choose/add-broadband/') === -1){
        return false;
    }
    var obj = JSON.parse(lsitem);
    var currentlySelected = false;
    obj.forEach(function(item){
        if(item.currentlySelected === true){
            currentlySelected = item.name;
        }
    });
    return currentlySelected;
}

$smcT5.CustomScripts.BroadbandFastestAvailable = function(){
    var lsitem = localStorage.getItem('smc_available_packages');
    if (!lsitem){
        return false;
    } else if (location.href.indexOf('/choose/add-broadband/') === -1){
        return false;
    }
    var obj = JSON.parse(lsitem);
    return obj[obj.length -1].name;
}

$smcT5.CustomScripts.BroadbandRuleSwitcher = function(){
    var fastestAvailable = $smcT5.CustomScripts.BroadbandFastestAvailable();
    var currentlySelected = $smcT5.CustomScripts.BroadbandCurrentlySelected();
    if (!fastestAvailable || !currentlySelected){
        return false;
    }
    if (currentlySelected === 'Essential Plus' && fastestAvailable === 'Superfast') {
        return 1;
    } else if (currentlySelected === 'Superfast 35' && fastestAvailable === 'Superfast') {
        return 1;
    } else if (currentlySelected === 'Superfast 35' && fastestAvailable === 'Ultrafast Plus') {
        return 2;
    } else if (currentlySelected === 'Superfast' && fastestAvailable === 'Ultrafast Plus') {
        return 3;
    } else if (currentlySelected === 'Ultrafast' && fastestAvailable === 'Ultrafast Plus') {
        return 4;
    } else if (currentlySelected === 'Ultrafast Plus' && fastestAvailable === 'Ultrafast Plus') {
        return 5;
    } else {
        return false;
    }
};

$smcT5.CustomScripts.SelectedSuperfastBroadband = function(){
    var selectedTab = document.querySelector('div[data-test-id="bbt-broadband-section"] button[aria-selected="true"]');
    if(selectedTab) {
        var title = selectedTab.getAttribute('aria-label').toLowerCase();
        if(title.indexOf('superfast') > -1) {
            $smcT5.CookieManager.create('smc_sele_supfast', true, 0);
        } else {
            $smcT5.CookieManager.del('smc_sele_supfast');
        }
    }
    var cookie = $smcT5.CookieManager.read('smc_sele_supfast');
    return cookie ? cookie : false;
};

